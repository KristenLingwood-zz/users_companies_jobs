{
  "$id": "http://localhost:3000/jobs",
  "type": "object",
  "additionalProperties": false,
  "properties": {
    "title": {
      "$id": "/properties/title",
      "type": "string",
      "title": "The Title Schema ",
      "default": "",
      "examples": ["engineer"]
    },
    "salary": {
      "$id": "/properties/salary",
      "type": "string",
      "title": "The Salary Schema ",
      "default": "",
      "examples": ["1000000"]
    },
    "equity": {
      "$id": "/properties/equity",
      "type": "number",
      "title": "The Equity Schema ",
      "default": 0,
      "examples": [9.800000190734863]
    },
    "company_id": {
      "$id": "/properties/company_id",
      "type": "integer",
      "title": "The Company_id Schema ",
      "default": 0,
      "examples": [2]
    }
  },
  "required": ["title", "company_id"]
}
